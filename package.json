{
  "name": "desops-pipeline",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "brandai-download": "echo \"Downloading Brand.ai assets...\" && npm run brandai-assets && echo \"Processing SVG icons...\" && npm run process-icons",
    "brandai-assets": "npm run clean-assets && node fetch-brandai-assets.js",
    "process-icons": "svg-sprite -h 18 -w 18 -sD src/assets/tmp --symbol-dest . --sscss src/assets/tmp/*.svg --ss ../scss/icons/_icons-brandai.scss.svg --symbol-render-scss-dest ../scss/icons/_icons-brandai.scss && rimraf src/assets/tmp",
    "clean-assets": "npm run clean-icons && npm run clean-logos && npm run clean-styles",
    "clean-icons": "rimraf src/assets/scss/icons/_icons-brandai.scss && rimraf src/assets/scss/icons/_icons-brandai.scss.svg",
    "clean-logos": "rimraf src/assets/imgs/logos",
    "clean-styles": "rimraf src/assets/scss/variables/_variables-brandai.scss",
    "postinstall": "npm run brandai-download"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "core-js": "^2.4.1",
    "reset-css": "^2.2.1",
    "rxjs": "^5.4.2",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.4.3",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.1.1",
    "download": "^6.2.5",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "svg-sprite": "^1.3.7",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
}
